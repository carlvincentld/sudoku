		"prepack": "webpack --config webpack.config.prod.js"
	},
	"devDependencies": {
		"mocha": "^7.0.0",
		"npm-run-all": "^4.1.5",
		"ts-loader": "^9.3.1",
		"typescript": "^4.8.2",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0",
		"webpack-merge": "^5.8.0"
	}
}
